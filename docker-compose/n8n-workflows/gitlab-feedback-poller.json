      "name": "Loop MRs",
      "type": "n8n-nodes-base.splitInBatches",
      "typeVersion": 3,
      "position": [1240, 200]
    },
    {
      "parameters": {
        "method": "GET",
        "url": "={{ $env.GITLAB_URL }}/api/v4/projects/{{ $('Loop Projects').item.json.gitlabPath | encodeURIComponent }}/merge_requests/{{ $json.iid }}/notes",
        "authentication": "predefinedCredentialType",
        "nodeCredentialType": "gitlabApi",
        "options": {
          "qs": {
            "per_page": 100
          }
        }
      },
      "id": "get-mr-notes",
      "name": "Get MR Notes",
      "type": "n8n-nodes-base.httpRequest",
      "typeVersion": 4.2,
      "position": [1440, 200],
      "notes": "MRÏùò Î™®Îì† ÏΩîÎ©òÌä∏ Ï°∞Ìöå"
    },
    {
      "parameters": {
        "jsCode": "// AI ÏΩîÎ©òÌä∏ ÏãùÎ≥Ñ\nconst notes = $input.all();\nconst projectId = $('Loop Projects').item.json.gitlabPath;\nconst mrIid = $('Loop MRs').item.json.iid;\n\nconst aiNotes = [];\n\nfor (const item of notes) {\n  const note = item.json;\n  \n  // AI ÏΩîÎ©òÌä∏ ÏãùÎ≥Ñ (authorÍ∞Ä Î¥áÏù¥Í±∞ÎÇò ÌäπÏ†ï ÎßàÏª§Í∞Ä ÏûàÎäî Í≤ΩÏö∞)\n  const isAiComment = note.body && (\n    note.body.includes('## ü§ñ AI Code Review') ||\n    note.body.includes('Claude Flow') ||\n    note.author?.username === 'claude-bot'\n  );\n  \n  if (isAiComment) {\n    aiNotes.push({\n      projectId: projectId,\n      mrIid: mrIid,\n      noteId: note.id\n    });\n  }\n}\n\nreturn aiNotes.map(n => ({ json: n }));"
      },
      "id": "find-ai-notes",
      "name": "Find AI Notes",
      "type": "n8n-nodes-base.code",
      "typeVersion": 2,
      "position": [1640, 200],
      "notes": "AI ÏΩîÎ©òÌä∏Îßå ÌïÑÌÑ∞ÎßÅ"
    },
    {
      "parameters": {
        "conditions": {
          "options": {
            "caseSensitive": true,
            "leftValue": "",
            "typeValidation": "strict"
          },
          "conditions": [
            {
              "id": "has-ai-notes",
              "leftValue": "={{ $input.all().length }}",
              "rightValue": 0,
              "operator": {
                "type": "number",
                "operation": "gt"
              }
            }
          ],
          "combinator": "and"
        },
        "options": {}
      },
      "id": "has-ai-notes",
      "name": "Has AI Notes?",
      "type": "n8n-nodes-base.if",
      "typeVersion": 2,
      "position": [1840, 200]
    },
    {
      "parameters": {
        "batchSize": 1,
        "options": {}
      },
      "id": "loop-ai-notes",
      "name": "Loop AI Notes",
      "type": "n8n-nodes-base.splitInBatches",
      "typeVersion": 3,
      "position": [2040, 100]
    },
    {
      "parameters": {
        "method": "GET",
        "url": "={{ $env.GITLAB_URL }}/api/v4/projects/{{ $json.projectId | encodeURIComponent }}/merge_requests/{{ $json.mrIid }}/notes/{{ $json.noteId }}/award_emoji",
        "authentication": "predefinedCredentialType",
        "nodeCredentialType": "gitlabApi",
        "options": {}
      },
      "id": "get-note-emojis",
      "name": "Get Note Emojis",
      "type": "n8n-nodes-base.httpRequest",
      "typeVersion": 4.2,
      "position": [2240, 100],
      "notes": "AI ÏΩîÎ©òÌä∏Ïùò Ïù¥Î™®ÏßÄ Ï°∞Ìöå"
    },
    {
      "parameters": {
        "jsCode": "// ÌîºÎìúÎ∞± Ïù¥Î™®ÏßÄ ÌïÑÌÑ∞ÎßÅ (üëç/üëé)\nconst emojis = $input.first().json;\nconst noteData = $('Loop AI Notes').item.json;\nconst feedbackEmojis = [];\n\nif (Array.isArray(emojis)) {\n  for (const emoji of emojis) {\n    if (['thumbsup', 'thumbsdown', '+1', '-1'].includes(emoji.name)) {\n      feedbackEmojis.push({\n        projectId: noteData.projectId,\n        noteId: noteData.noteId,\n        emoji: emoji.name,\n        userId: emoji.user?.id || 0,\n        action: 'award'\n      });\n    }\n  }\n}\n\nreturn feedbackEmojis.map(e => ({ json: e }));"
      },
      "id": "filter-feedback-emojis",
      "name": "Filter Feedback Emojis",
      "type": "n8n-nodes-base.code",
      "typeVersion": 2,
      "position": [2440, 100],
      "notes": "üëç/üëé Ïù¥Î™®ÏßÄÎßå ÌïÑÌÑ∞ÎßÅ"
    },
